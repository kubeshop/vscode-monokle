{
  "name": "monokle-vsc",
  "displayName": "Monokle",
  "description": "Monokle Visual Studio Code extension for validating Kubernetes resources.",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.79.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [
    "onStartupFinished",
    "onFileSystem:file"
  ],
  "main": "./out/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "monokle-vsc.validate",
        "title": "Monokle: Validate"
      }
    ]
  },
  "extensionDependencies": [
    "ms-sarifvscode.sarif-viewer"
  ],
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "tsc -p ./",
    "watch": "tsc -watch -p ./",
    "pretest": "npm run compile && npm run lint",
    "lint": "eslint src --ext ts",
    "test": "node ./out/test/runTest.js"
  },
  "devDependencies": {
    "@types/glob": "^8.1.0",
    "@types/mocha": "^10.0.1",
    "@types/node": "20.2.5",
    "@types/vscode": "^1.79.0",
    "@typescript-eslint/eslint-plugin": "^5.59.8",
    "@typescript-eslint/parser": "^5.59.8",
    "@vscode/test-electron": "^2.3.2",
    "eslint": "^8.41.0",
    "glob": "^8.1.0",
    "mocha": "^10.2.0",
    "typescript": "^5.1.3"
  },
  "dependencies": {
    "@monokle/validation": "^0.23.4",
    "uuid": "^9.0.0"
  }
}
